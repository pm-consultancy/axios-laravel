{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "strict": false,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true,
    "skipDefaultLibCheck": true
  },
}
