{
  "name": "pmc-laravel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.2.47",
    "vuetify": "^3.1.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.16.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "typescript": "^4.9.3",
    "unbuild": "^1.2.0",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vuetify": "^3.1.15"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ]
}
